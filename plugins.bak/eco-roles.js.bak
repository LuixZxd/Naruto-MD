export function obtenerRango(level) {
  const entradas = Object.entries(rangos).sort((a, b) => a[1] - b[1])
  let rangoEncontrado = 'Sin rango asignado'
  for (let [nombre, minLevel] of entradas) {
    if (level >= minLevel) rangoEncontrado = nombre
    else break
  }
  return rangoEncontrado
}